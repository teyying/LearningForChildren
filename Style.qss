/******************************************************** 登录界面 ***************************************/
/*调y1的浮点值：改变从上至下透明区域的高度。调stop:1中的a值：改变透明度值。其它参数还不太明白*/
QWidget#Login QWidget#widLogin {background-color: qlineargradient(spread:pad, x1:1, y1:0.3, x2:1, y2:0.1, stop:0 rgba(205, 230, 199, 250), stop:1 rgba(205, 230, 199, 200));}
QWidget#Login QWidget#widUserpic {border:2px solid white;border-radius:37px;}
QWidget#Login QLabel#labUserpic {border-radius:35px;border-image: url("image/userPic/default.png");}

QWidget#Login QLabel#labLoginError {color: red;}


QPushButton#btnMini, QPushButton#btnExit {background: transparent;color:#A6A6A6;}
QPushButton#btnMini:hover {background: rgba(100, 100, 100, 50);border-style:outset;}

QWidget#Login QPushButton#btnExit:hover {background: #FF5439;border-style:outset;}
QWidget#Login QPushButton#btnVipLogin,QPushButton#btnVisitorLogin {background-color: #02B8FA;border-radius:3px;color:#DEF7FF;}
QWidget#Login QPushButton#btnVipLogin:hover,QPushButton#btnVisitorLogin:hover {background-color: #1FC7FD;}

QWidget#Login QPushButton#btnSignIn{background: transparent;color:#A6A6A6;}
QWidget#Login QPushButton#btnSignIn:hover{color:#838383;}
QWidget#Login QCheckBox#cBoxAotuLogin,QCheckBox#cBoxKeepPswd {color:#A6A6A6;}
QWidget#Login QCheckBox#cBoxAotuLogin:hover,QCheckBox#cBoxKeepPswd:hover {color:#838383;}

QWidget#Login QLineEdit, QComboBox {background: transparent;border: 1px solid #CDE6C7;border-bottom: 1px solid #A6A6A6;color:#838383;font-size:13px;}
QWidget#Login QLineEdit:hover,QComboBox:hover {border-bottom: 1px solid #838383;}
QWidget#Login QLineEdit:focus,QComboBox:focus {border-bottom: 1px solid #02B8FA;}


QWidget#Login QComboBox:drop-down {width: 32px;border-left: 1px solid #CDE6C7;}  /*右边添加分割块:宽度,线宽度,样式,颜色*/
QWidget#Login QComboBox:down-arrow {border-image:url("UiLogin/image/5.png");}    /*的箭头样式*/

QWidget#Login QComboBox::down-arrow:hover {border-image: url("UiLogin/image/6.png");}  /*鼠标移入的效果*/
QWidget#Login QComboBox::down-arrow:on {border-image: url("UiLogin/image/7.png");}     /*鼠标点击的效果*/
/*下拉列表项:字体样式, 背景*/
QWidget#Login QComboBox QAbstractItemView {

        outline:0px;             /*去掉下拉框虚线*/
        background: transparent; /*这里设置透明背景只变成深灰色,不设置是白色*/
}

/*程序中写一个self.comboUserID.setView(QListView())就可以设置下拉列表的样式了*/
QWidget#Login QComboBox QAbstractItemView::item {
        min-height: 40px;            /*列表高度*/
        min-width: 60px;             /*列表宽度*/
        background-color:white; /*列表背景色*/
        font:60px;
        font-family: 华文宋体;
        font-weight:300;
        color: gray;                   /*字体的颜色*/
        selection-color:black;         /*选择时显示的字体颜色*/
}
QWidget#Login QComboBox QAbstractItemView::item:hover {
        border: 1px solid gray;   /*列表边框宽度,样式,颜色*/
        background-color: lightgray;   /*滑过当前列表项的背景色*/
}





